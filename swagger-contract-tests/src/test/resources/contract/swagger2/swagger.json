                    {
                        "petstore_auth": [
                            "write:pets",
                            "read:pets"
                        ]
                    }
                ],
                "deprecated": true
            }
        },
        "/api/pet/{petId}": {
            "get": {
                "tags": [
                    "pet-controller"
                ],
                "summary": "Find pet by ID",
                "description": "Returns a pet when ID < 10. ID > 10 or nonintegers will simulate API error conditions",
                "operationId": "getPetByIdUsingGET_5",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/xml",
                    "application/json"
                ],
                "parameters": [
